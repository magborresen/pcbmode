  "create": {
    "docs": true,
    "drill-index": true,
    "flashes": true,
    "layer-index": true
  },
  "params": {
    "units": "mm",
    "significant-digits": 6,
    "display-frame-buffer": 1.0,
    "num-of-digest-digits": 12,
    "invert-y-axis": true
  },
  "distances": {
    "from-pour-to": {
      "outline": 0.5,
      "drill": 1.0,
      "pad": 0.2,
      "route": 0.25
    },
    "soldermask": {
      "path-scale": 1.05,
      "rect-buffer": 0.05,
      "circle-buffer": 0.05
    },
    "solderpaste": {
      "path-scale": 0.9,
      "rect-buffer": -0.1,
      "circle-buffer": -0.1
    }
  },
  "gerber": {
    "decimals": 6,
    "digits": 6,
    "min-segment-length": 0.05,
    "steps-per-segment": 100
  },
